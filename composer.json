{
    "name": "skorelabs/nova-panels",
    "description": "A custom panels package for those Nova resources with lots of relationships and/or fields",
    "license": "MIT",
    "authors": [
        {
            "name": "Rubén Robles",
            "email": "d8vjork@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "illuminate/support": "^8.0",
        "laravel/nova": "^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SkoreLabs\\NovaPanels\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SkoreLabs\\NovaPanels\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
